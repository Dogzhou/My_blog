<%= form_tag users_path do %>
  <p>
    <%= label_tag :名字 %><br />
    <%= text_field_tag :name %>
  </p>
  <p>
    <%= label_tag :性别 %><br />
    <%= text_field_tag :sex %>
  </p>
  <p>
    <%= label_tag :出生日期 %><br />
    <%= date_select :sex, options = {} %>
  </p>
  <p>
    <%= label_tag :婚姻情况 %><br />
    <%= text_field_tag :status %>
  </p>
  <p>
    <%= label_tag :身高 %><br />
    <%= number_field_tag :height %>
    </p>
    <%= submit_tag "确认" %>
<% end %>
