<%= form_tag(user_zhuce_path, method: :post) do %>
  <p>
    <%= label_tag :用户名 %><br />
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :密码 %><br />
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= label_tag :确认密码 %><br />
    <%= password_field_tag :password_confirmation %>
  </p>
  <p><%= submit_tag "注册" %></p>
<% end %>
